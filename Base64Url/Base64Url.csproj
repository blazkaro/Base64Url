<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<Nullable>enable</Nullable>
		<Authors>Karol Błażejczyk</Authors>
		<Description>A simple implementation of Base64-Url encoding</Description>
		<Copyright>Copyright © 2022 Karol Błażejczyk</Copyright>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/blazkaro/Base64Url</PackageProjectUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/blazkaro/Base64Url</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>base64url;encoding;encoder;base64;decoding;decoder;validator</PackageTags>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<Title>Base64Url encoder, decoder and validator</Title>
		<NeutralLanguage>en</NeutralLanguage>
		<Version>1.0.0</Version>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>
